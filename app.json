{
  "expo": {
    "name": "NoArts!",
    "slug": "noarts-app",
    "scheme": "noarts",
    "privacy": "unlisted",
    "platforms": ["ios", "android"],
    "version": "0.8.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4e6864"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "useNextNotificationsApi": true,
      "package": "net.hartmann_dev.naapp",
      "versionCode": 512,
      "permissions": ["NOTIFICATIONS"],
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.hartmann-dev.naapp",
      "buildNumber": "512",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": "No"
      }
    },
    "description": "",
    "extra": {
      "apiUrlDev": "https://noarts.tripoint.app",
      "apiUrlProd": "https://noarts.tripoint.app"
    }
  }
}
